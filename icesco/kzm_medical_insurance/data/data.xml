<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_medical_record" model="ir.sequence">
            <field name="name">Medical Record</field>
            <field name="code">medical.record</field>
            <field name="prefix">DM</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_medical_refund_request" model="ir.sequence">
            <field name="name">Medical refund request</field>
            <field name="code">medical.refund.request</field>
            <field name="prefix">DR</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_medical_refund_run_request" model="ir.sequence">
            <field name="name">Medical refund request run</field>
            <field name="code">medical.refund.request.run</field>
            <field name="prefix">LDR</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_medical_medical_contract" model="ir.sequence">
            <field name="name">medical contract</field>
            <field name="code">medical.contract</field>
            <field name="prefix">CM</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

