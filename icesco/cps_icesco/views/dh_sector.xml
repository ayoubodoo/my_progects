<odoo>
    <data>
        <record model="ir.ui.view" id="dh_sector_form">
            <field name="name">dh.sector.form</field>
            <field name="model">dh.sector</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="dh_sector_tree">
            <field name="name">dh.sector.tree</field>
            <field name="model">dh.sector</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name" />
                </tree>
            </field>
        </record>

        <record id="dh_sector_action" model="ir.actions.act_window">
            <field name="name"> Sctor </field>
            <field name="res_model">dh.sector</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
            <p>Add a sector </p>
        </field>
        </record>

<!--        <menuitem id="sectors_menu" name="Sectors"-->
<!--                  parent="project.menu_project_config"-->
<!--                  action="cps_icesco.dh_sector_action" sequence="4"/>-->




    </data>
</odoo>