<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="cps_view_user_form" model="ir.ui.view">
        <field name="name">res.user.cps.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form" />
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/notebook" position="inside">
                <page string="Parametrages Users" name="icesco">
                    <group>
                        <field name="is_dg"/>
                        <field name="is_dpt_participation"/>
                        <field name="is_translation"/>
                        <field name="is_design"/>
                        <field name="is_legal"/>
                        <field name="is_logistics"/>
                        <field name="is_protocol"/>
                        <field name="is_finance"/>
                        <field name="is_admin"/>
                        <field name="is_it"/>
                        <field name="is_media"/>
                        <field name="is_coverage"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>