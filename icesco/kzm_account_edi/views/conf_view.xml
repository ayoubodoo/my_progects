		<record id="liasse_extra_view_form_erp" model="ir.ui.view">
			<field name="name">Extra field</field>
			<field name="model">liasse.extra.field.erp</field>
			<field name="arch" type="xml">
				<form string="Extra field">
					<group col="4" colspan="4" string="Detail cpc">
						<field name="code0cpc" />
					</group>
					<group col="4" colspan="4" string="Provision">
						<field name="code1prov" />
					</group>
					<group col="4" colspan="4" string="Fusion">
						<field name="code2fusion" />
					</group>
				</form>
			</field>
		</record>

		<record id="liasse_conf_view_form_erp" model="ir.ui.view">
			<field name="name">Configuration</field>
			<field name="model">liasse.configuration.erp</field>
			<field name="arch" type="xml">
				<form string="Configuration">
					<separator string="Association des codes EDI" />
					<group col="4" colspan="4">
						<field name="code" />
						<field name="type" />
						<field name="compte"
							attrs="{'invisible': [('type','in',['1','2'])],'required':[('type','not in',['1','2','3','4'])]}">
							<tree editable="bottom">
								<field name="compte"></field>
							</tree>
						</field>
						<field name="comptem" string="Comptes exclus"
							attrs="{'invisible': [('type','in',['1','2'])],'required':[('type','not in',['1','2','3','4'])]}">
							<tree editable="bottom">
								<field name="compte"></field>
							</tree>
						</field>
					</group>
					<group attrs="{'invisible': [('type','in',['0','5','6'])]}"
						col="4" colspan="4">

						<field name="code_ids" attrs="{'required':[('type','in',['1','2','3','4'])]}"
							widget="many2many_tags" string="Somme" />
						<field name="code_min_ids" attrs="{'invisible': [('type','in',['1','3','4'])]}"
							widget="many2many_tags" string="Minus" />
					</group>
				</form>
			</field>
		</record>

