		<record id="liasse_passage_view_form_erp" model="ir.ui.view">
			<field name="name">Passage</field>
			<field name="model">liasse.passage.erp</field>
			<field name="arch" type="xml">
				<form string="Interet">
					<group string="RESULTAT NET COMPTABLE" col="4" colspan="4">
						<field name="code0" />
						<field name="code1" />
						<field name="code2" />
						<field name="code3" />
					</group>
					<group string="REINTEGRATIONS FISCALES COURANTES">
						<field name="passages_rfc" nolabel="1" context="{'type_in':'0'}">
							<tree string="Intertes" editable="bottom">
								<field name="code0" />
								<field name="code1" required="True" />
								<!-- <field name="code2" /> -->
							</tree>
						</field>
					</group>
					<group string="REINTEGRATIONS FISCALES NON COURANTES">
						<field name="passages_rfnc" nolabel="1" context="{'type_in':'1'}">
							<tree string="Intertes" editable="bottom">
								<field name="code0" />
								<field name="code1" required="True" />
								<!-- <field name="code2" /> -->
							</tree>
						</field>
					</group>
					<group string="DEDUCTIONS FISCALES COURANTES">
						<field name="passages_dfc" nolabel="1" context="{'type_in':'2'}">
							<tree string="Intertes" editable="bottom">
								<field name="code0" />
								<!-- <field name="code1" /> -->
								<field name="code2" required="True" />
							</tree>
						</field>
					</group>
					<group string="DEDUCTIONS FISCALES NON COURANTES">
						<field name="passages_dfnc" nolabel="1" context="{'type_in':'3'}">
							<tree string="Intertes" editable="bottom">
								<field name="code0" />
								<!-- <field name="code1" /> -->
								<field name="code2" required="True" />
							</tree>
						</field>
					</group>
					<separator string="Total" />
					<group col="4" colspan="4">
						<field name="code4" />
						<field name="code5" />
					</group>
					<group string="RESULTAT BRUT FISCAL" col="4" colspan="4">
						<field name="code6" />
						<field name="code7" />
						<field name="code8" />
						<field name="code9" />
					</group>
